<!doctype html>
<html>
<meta charset="utf-8">
<body>
<h1>Listado de contactos</h1>
{% if contactos %}
    {% for contacto in contactos %}
        {{ include('partials/datos_contacto.html.twig', {'contacto': contacto}) }}
    {% endfor %}
{% else %}
    <h4>Contacto no encontrado</h4>
{% endif %}
</body>
</html>
